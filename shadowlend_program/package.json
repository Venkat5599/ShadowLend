{
  "name": "shadowlend-program",
  "version": "0.1.0",
  "license": "MIT",
  "description": "ShadowLend Protocol - Confidential DeFi Lending on Solana with Arcium MXE",
  "scripts": {
    "lint:fix": "prettier */*.js \"*/**/*{.js,.ts}\" -w",
    "lint": "prettier */*.js \"*/**/*{.js,.ts}\" --check",
    "build": "anchor build",
    "deploy": "ANCHOR_PROVIDER_URL=https://api.devnet.solana.com ANCHOR_WALLET=~/.config/solana/id.json ARCIUM_CLUSTER_OFFSET=456 ts-node scripts/deploy.ts",
    "deploy:skip-comp": "ANCHOR_PROVIDER_URL=https://api.devnet.solana.com ANCHOR_WALLET=~/.config/solana/id.json ARCIUM_CLUSTER_OFFSET=456 ts-node scripts/deploy.ts --skip-comp-defs",
    "deploy:local": "ANCHOR_PROVIDER_URL=http://127.0.0.1:8899 ANCHOR_WALLET=~/.config/solana/id.json ts-node scripts/deploy.ts",
    "test:devnet": "ARCIUM_CLUSTER_OFFSET=456 ANCHOR_PROVIDER_URL=https://api.devnet.solana.com ANCHOR_WALLET=~/.config/solana/id.json ts-node scripts/test-devnet.ts",
    "verify": "ANCHOR_PROVIDER_URL=https://api.devnet.solana.com ANCHOR_WALLET=~/.config/solana/id.json ts-node scripts/verify-deployment.ts"
  },
  "dependencies": {
    "@arcium-hq/client": "0.6.3",
    "@coral-xyz/anchor": "^0.32.1",
    "@solana/spl-token": "^0.4.6",
    "@solana/web3.js": "^1.95.4",
    "chalk": "^4.1.2",
    "dotenv": "^17.2.3"
  },
  "devDependencies": {
    "@types/bn.js": "^5.1.0",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^20.10.0",
    "chai": "^4.3.4",
    "mocha": "^9.0.3",
    "prettier": "^2.6.2",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}

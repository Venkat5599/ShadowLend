{
  "name": "sdk",
  "version": "1.0.0",
  "description": "ShadowLend SDK for confidential lending on Solana",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && tsc-alias",
    "docs": "typedoc",
    "docs:open": "xdg-open docs/index.html",
    "format": "prettier --write .",
    "test:deposit": "ts-node -r tsconfig-paths/register --project tests/tsconfig.json tests/test-deposit.ts",
    "test:borrow": "ts-node -r tsconfig-paths/register --project tests/tsconfig.json tests/test-borrow.ts",
    "test:spend": "ts-node -r tsconfig-paths/register --project tests/tsconfig.json tests/test-spend.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@solana/kit": "^5.5.1",
    "@types/node": "^25.2.0",
    "prettier": "^3.8.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typedoc": "^0.28.16",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@arcium-hq/client": "^0.7.0",
    "@coral-xyz/anchor": "^0.32.1",
    "@solana/spl-token": "^0.4.14",
    "@solana/web3.js": "^1.98.4",
    "dotenv": "^17.2.3"
  }
}
